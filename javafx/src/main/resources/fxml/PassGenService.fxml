<AnchorPane fx:id="mainPane" maxHeight="680.0" maxWidth="1080.0" minHeight="680.0" minWidth="1080.0" prefHeight="680.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hhn.it.devtools.javafx.controllers.PassGenServiceController">
   <children>
      <Label fx:id="loginLabel" layoutX="503.0" layoutY="169.0" text="Login">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="tokenTextField" layoutX="460.0" layoutY="231.0" promptText="Type in Token" />
      <TextField fx:id="emailTextField" layoutX="460.0" layoutY="296.0" promptText="Type in Email" />
      <PasswordField fx:id="passwordTextField" layoutX="460.0" layoutY="340.0" promptText="Type in Password" />
      <Button fx:id="loginButton" layoutX="596.0" layoutY="391.0" mnemonicParsing="false" onAction="#clickLoginButton" text="Login" />
      <Button fx:id="registerButton" layoutX="429.0" layoutY="391.0" mnemonicParsing="false" text="Register" />
   </children>
</AnchorPane>
