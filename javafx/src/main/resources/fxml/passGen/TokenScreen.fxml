<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="tokenPane" maxHeight="680.0" maxWidth="1080.0" minHeight="680.0" minWidth="1080.0" prefHeight="680.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="tokenHeader" layoutX="508.0" layoutY="186.0" text="Token">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="tokenField" editable="false" layoutX="460.0" layoutY="264.0" text="gets set by application" />
      <Button fx:id="copyTokenButton" layoutX="516.0" layoutY="303.0" mnemonicParsing="false" onAction="#clickCopyToken" text="Copy" />
      <Button fx:id="advanceButton" layoutX="506.0" layoutY="340.0" mnemonicParsing="false" onAction="#clickAdvance" text="Advance" />
   </children>
</AnchorPane>
